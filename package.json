	},
	"devDependencies": {
		"@types/mapbox__polyline": "^1.0.5",
		"bun-types": "^1.0.0",
		"typescript": "^5.3.3"
	},
	"keywords": [
		"strava",
		"maplibre",
		"exploration",
		"geospatial",
		"web-worker",
		"indexeddb",
