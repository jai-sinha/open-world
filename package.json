{
	"name": "open-world-strava",
	"version": "0.1.0",
	"description": "Client-side MapLibre exploration map for Strava activities with Web Workers and IndexedDB",
	"type": "module",
	"homepage": "openworld.jsinha.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/jai-sinha/open-world.git"
	},
	"bugs": {
		"url": "https://github.com/jai-sinha/open-world/issues"
	},
	"author": "Jai Sinha <jaisinha219@gmail.com>",
	"license": "MIT",
	"engines": {
		"bun": "^1.0.0"
	},
	"scripts": {
		"dev": "bun run --hot server/server.ts",
		"build": "bun run build.ts",
		"prod": "bun run server/server.ts --prod",
		"preview": "bun run server/server.ts --prod",
		"type-check": "tsc --noEmit",
		"check": "bun run scripts/check-setup.ts"
	},
	"dependencies": {
		"@mapbox/polyline": "^1.2.1",
		"@turf/boolean-point-in-polygon": "^7.3.1",
